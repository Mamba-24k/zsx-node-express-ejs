        </div>
    </div>
</header>

<!--轮播图以及标签分类-->
<section class="banner">
    <div class="banner-top"></div>
    <!--轮播图-->
    <div class="swiper-container">
        <div class="swiper-wrapper">
            <div class="swiper-slide">
                <img src="./static2/img/carousel1.png" alt="易建采轮播图片">
            </div>
            <div class="swiper-slide">
                <img src="./static2/img/carousel2.jpg" alt="易建采轮播图片">
            </div>
            <div class="swiper-slide">
                <img src="./static2/img/carousel3.jpg" alt="易建采轮播图片">
            </div>
        </div>
    </div>
    <!--标签分类-->
    <div class="classify">
        <div class="content-box">
            <ul class="classify-top clearfix">
                <li class="classify-top-title">招标分类</li>
                <li><%= '采购项目总数：' + accumulatedData.totalPurchaserProjectNums %></li>
                <li><%= '成交总额：￥' + (accumulatedData.totalDeals/100000000).toFixed(2) %>亿+</li>
                <li><%= '注册会员：' + accumulatedData.totalUsers %></li>
            </ul>
            <section class="classify-main clearfix">
                <ul class="tag-left fl">
                  <% treeArray.forEach(item => { %>
                    <li>
                        <ul>
                          <% item.children.forEach(item2 => { %>
                            <li class="tag-list-item">
                                <div class="tag-list-title clearfix">
                                    <span class="fl"><%= item2.text %></span>
                                    <span class="fr">></span>
                                    <div class="tag-list-bg"></div>
                                </div>
                                <div class="tag-right">
                                    <ul class="level-box-1">
                                      <% item2.children.forEach(item3 => { %>
                                        <li class="level-1 clearfix">
                                            <p class="level-title fl"><a
                                                href="<%= address %>/biddingAnnouncement?purchaseClass=<%= item.text == '货物采购' ? 1 : item.text == '工程项目' ? 2 : 3 %>&tagId=<%= item3.id %>"><%= item3.text %></a>
                                            </p>
                                            <ul class="level-box-2 fl clearfix">
                                              <% item3.children.forEach(item4 => { %>
                                                <li class="level-2"><a
                                                    href="<%= address %>/biddingAnnouncement?purchaseClass=<%= item.text == '货物采购' ? 1 : item.text == '工程项目' ? 2 : 3 %>&tagId=<%= item4.id %>"><%= item4.text %></a>
                                                </li>
                                              <% }) %>
                                            </ul>
                                        </li>
                                      <% }) %>
                                    </ul>
                                </div>
                            </li>
                          <% }) %>
                        </ul>
                    </li>
                  <% }) %>
                </ul>
            </section>
        </div>
    </div>
</section>

<!--项目列表-->
<section>
    <div class="content-box overflow">
        <!--四个格子-->
        <ul class="grid clearfix top-60">
            <li class="fl grid-item">
                <div class="grid-item-bg"></div>
                <a href="<%= address %>/bidInfo" class="grid-item-text">招标投标</a>
            </li>
            <li class="fl grid-item">
                <div class="grid-item-bg"></div>
                <a href="<%= address %>/supplierSearch" class="grid-item-text">一键询价</a>
            </li>
            <li class="fl grid-item">
                <div class="grid-item-bg"></div>
                <a href="<%= address %>/biddingAnnouncement?purchaseMode=cPurchaser" class="grid-item-text">竞价采购</a>
            </li>
            <li class="fl grid-item">
                <div class="grid-item-bg"></div>
                <a href="<%= address %>/biddingAnnouncement?purchaseMode=iInquiry"
                   class="grid-item-text">询比采购</a>
            </li>
        </ul>

        <!--非招标项目-->
        <section class="project top-60">
            <header class="project-type-box clearfix">
                <h2 class="project-type fl">非招标项目</h2>
                <span class="more fr"><a href="<%= address %>/biddingAnnouncement">更多>></a></span>
            </header>
            <ul class="project-list clearfix">
                <% privateList.forEach(item => { %>
                  <li class="project-list-item">
                   <div class="project-name clearfix">
                      <h3 class="over-ellipsis fl"><a
                          href="<%= address %>/biddingAnnouncementDetails?purchaseMode=<%= item.projectType %>&projectId=<%= item.projectId %>&purchasingInformationId=<%= item.projectTypeId %>&isEnd=<%= item.projectType == 'iInquiry' ? item.inquiryInfo.isEnd : item.projectType == 'cPurchaser' ? item.contentInfo.isEnd : item.projectType == 'epurchase' ? item.epurchaserInfo.isEnd : item.tenderInfo.isEnd %>"><%= item.projectName %></a>
                      </h3>
                        <i
                          class="fr text-icon-<%= item.projectType == 'iInquiry' ? 3 :item.projectType == 'cPurchaser' ? 2 : item.projectType == 'epurchase' ? 1 : 0 %>">
                        </i>
                    </div>
                    <div class="project-info clearfix">
                        <p class="fl">
                            <span class="label-color">采购单位：</span>
                            <span class="val-color"><%= item.companyName %></span>
                        </p>
                    </div>
                    <div class="project-info clearfix">
                        <p class="fl">
                            <span class="label-color">采购分类：</span>
                            <span class="val-color"><%= item.inquiryInfo.procurementClassification %></span>
                        </p>
                        <p class="fr">
                            <span class="label-color">截止报名：</span>
                            <span class="val-time"><%= item.inquiryInfo.signDate %></span>
                        </p>
                    </div>
                </li> 
                <% }) %>
            </ul>
        </section>

        <!--招标项目-->
        <section class="project top-60">
            <header class="project-type-box tender clearfix">
                <h2 class="project-type fl">招标项目</h2>
                <span class="more fr"><a href="<%= address %>/bidInfo">更多>></a></span>
            </header>
            <ul class="project-list clearfix">
              <% publicList.forEach(item => { %>
                <li class="project-list-item">
                    <div class="project-name clearfix">
                        <h3 class="over-ellipsis fl"><a
                            href="<%= address %>/biddingAnnouncementDetails?purchaseMode=bidTender&tenderBulletinCode=<%= item.tenderBulletinCode %>&bidType=<%= item.tenderMode %>&isEnd=<%= item.isEnd %>"><%= item.tenderProjectName %></a>
                        </h3>
                        <i class="text-icon-1 fr"></i>
                    </div>
                    <div class="project-info clearfix">
                        <p class="fl">
                            <span class="label-color">招标单位：</span>
                            <span class="val-color"><%= item.tendererName %></span>
                        </p>
                    </div>
                    <div class="project-info clearfix">
                        <p class="fl">
                            <span class="label-color">公告类型：</span>
                            <span class="val-color">招标公告</span>
                        </p>
                        <p class="fr">
                            <span class="label-color">截止报名：</span>
                            <span class="val-time"><%= item.bulletinEndTime %></span>
                        </p>
                    </div>
                </li>
               <% }) %>
            </ul>
        </section>
    </div>
</section>

